PDBID="$1"
DIR="$(mktemp -d)"
trap "rm -r $DIR" EXIT
cd "$DIR"
curl "https://files.rcsb.org/download/${PDBID}.pdb1.gz" | zcat > "input.pdb"
[ -s "input.pdb" ] || { echo "Nothing downloaded."; exit 1; }
v-load -file ${DIR}/input.pdb -as-assembly
v-construct-contacts
v-voromqa-global
v-voromqa-local -contacts '[-inter-chain]'
